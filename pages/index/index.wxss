.banners {
  width: 100%;
    height:300rpx
}
.banners image {
  height:100%;
  width: 100%
}

/*五个图标导航区域*/
.navContainer {
  display: flex;
  justify-content: space-around
}

.navItem {
  display: flex;
  flex-direction: column;
  align-items: center
}

.iconfont {
  width:100rpx;
  height:100rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 100rpx;
  background-color: rgb(240, 19, 19);
  font-size: 50rpx;
  color: white;
  margin: 20rpx 0
}
.navItem>text:last-child {
  font-size: 26rpx
}

.recommandContainer{
  padding:20rpx 
}
.recommandContainer .header .title {
  font-size: 32rpx;
  line-height: 80rpx;
  color: #666
}

.recommandContainer .header .more{
  float: right;
  border: 1px solid #333;
  padding: 20rpx 30rpx;
  font-size: 26rpx;
}
